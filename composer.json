{
    "name": "affordablemobiles/eloquent-sqlcommenter",
    "description": "sqlcommenter support for Laravel's Eloquent ORM",
    "license": "MIT",
    "authors": [
        {
            "name": "Samuel Melrose",
            "email": "sam.melrose@affordablemobiles.co.uk"
        }
    ],
    "require": {
        "php": "^8.3",
        "open-telemetry/sdk": "~1",
        "illuminate/support": "~12",
        "illuminate/database": "~12",
        "symfony/http-client": "^7.2",
        "nyholm/psr7": "^1.8"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "AffordableMobiles\\sqlcommenter\\": "src/AffordableMobiles/sqlcommenter/"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
